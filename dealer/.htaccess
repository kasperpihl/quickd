RewriteEngine On
RewriteRule ^api/(login|register|subscribe|fbconnect|logout) ajax.php?action=$1 [NC,L]
RewriteRule ^api/shopowner/(templates|shops|feedback|images)/([0-9]{1,3})$ ajax.php?type=$1&id=$2 [NC,L]
RewriteRule ^api/shopowner/deals/([0-9a-fA-F]{32})$ ajax.php?type=deals&id=$1 [NC,L]
RewriteRule ^api/shopowner/(deals|templates|shops|feedback|images|all)$ ajax.php?type=$1 [NC,L]
RewriteCond %{REQUEST_FILENAME} !-f
RewriteCond %{REQUEST_FILENAME} !-d
RewriteCond %{REQUEST_URI} !index
RewriteRule (.*) index.php [NC,L]
